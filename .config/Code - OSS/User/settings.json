{
  "workbench.iconTheme": "material-icon-theme",
  "latex-workshop.chktex.enabled": true,
  "latex-workshop.intellisense.package.enabled": true,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.cursorBlinking": "phase",
  "window.zoomLevel": 0,
  "editor.quickSuggestionsDelay": 0,
  "latex-workshop.latex.clean.fileTypes": [
    "*.aux",
    "*.bbl",
    "*.blg",
    "*.idx",
    "*.ind",
    "*.lof",
    "*.lot",
    "*.out",
    "*.toc",
    "*.acn",
    "*.acr",
    "*.alg",
    "*.glg",
    "*.glo",
    "*.gls",
    "*.fls",
    "*.log",
    "*.fdb_latexmk",
    "*.snm",
    "*.synctex(busy)",
    "*.synctex.gz(busy)",
    "*.nav",
    "*.synctex.gz"
  ],
  "files.exclude": {
    "**/.directory": true
  },
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "editor.renderControlCharacters": false,
  "terminal.integrated.inheritEnv": true,
  "workbench.colorCustomizations": {
    "activityBarBadge.background": "#7abef5",
    "activityBar.activeBorder": "#f5984c",
    "list.activeSelectionForeground": "#f5984c",
    "list.inactiveSelectionForeground": "#f5984c",
    "list.highlightForeground": "#f5984c",
    "scrollbarSlider.activeBackground": "#f5984c50",
    "editorSuggestWidget.highlightForeground": "#f5984c",
    "textLink.foreground": "#f5984c",
    "progressBar.background": "#f5984c",
    "pickerGroup.foreground": "#f5984c",
    "notificationLink.foreground": "#f5984c",
    "editorWidget.resizeBorder": "#f5984c",
    "editorWidget.border": "#f5984c",
    "settings.modifiedItemIndicator": "#f5984c",
    "settings.headerForeground": "#f5984c",
    "panelTitle.activeBorder": "#f5984c",
    "breadcrumb.activeSelectionForeground": "#f5984c",
    "menu.selectionForeground": "#f5984c",
    "menubar.selectionForeground": "#f5984c",
    "editor.findMatchBorder": "#f5984c",
    "selection.background": "#f5984c40",
    "statusBarItem.remoteBackground": "#f5984c",
    "tab.activeBorder": "#f5984c",
    "tab.activeModifiedBorder": "#f5984c",
    "tab.inactiveModifiedBorder": "#b08058",
    "gitDecoration.modifiedResourceForeground": "#b0d0eb",
    "gitDecoration.addedResourceForeground": "#4ca9f5",
    "gitDecoration.stageModifiedResourceForeground": "#66b06b",
    "gitDecoration.deletedResourceForeground": "#f57067",
    "gitDecoration.stageDeletedResourceForeground": "#678ff5",
    "gitDecoration.untrackedResourceForeground": "#f5e48e",
    "[Community Material Theme Darker High Contrast]": {
      "editor.background": "#1f1f1f",
      "editor.foreground": "#ebebeb"
    },
    "tab.border": "#141414",
    "tab.hoverForeground": "#f5984c",
    "editorGroupHeader.tabsBackground": "#141414",
    "tab.activeBackground": "#141414",
    "tab.activeForeground": "#ebebeb",
    "tab.inactiveForeground": "#b0b0b0",
  },
  "editor.fontLigatures": true,
  "editor.hover.delay": 142,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.tabSizing": "shrink",
  "workbench.editor.tabCloseButton": "off",
  "editor.minimap.maxColumn": 42,
  "diffEditor.ignoreTrimWhitespace": false,
  "latex-workshop.message.error.show": false,
  "window.menuBarVisibility": "toggle",
  "workbench.statusBar.visible": true,
  "workbench.activityBar.visible": true,
  "latex-workshop.view.pdf.viewer": "external",
  "editor.fontSize": 16,
  "git.enableSmartCommit": true,
  "terminal.integrated.fontFamily": "Hack Nerd Font",
  "latex-workshop.latex.tools": [
    {
      "name": "latexmk",
      "command": "latexmk",
      "args": [
        "-synctex=1",
        "-interaction=nonstopmode",
        "-file-line-error",
        "-lualatex",
        "%DOC%"
      ]
    }
  ],
  "latex-workshop.synctex.afterBuild.enabled": true,
  "editor.fontFamily": "'Hack Nerd Font'",
  "workbench.colorTheme": "Community Material Theme Darker High Contrast",
  "terminal.integrated.cursorStyle": "line",
  "terminal.explorerKind": "external",
  "terminal.external.linuxExec": "kitty",
  "editor.mouseWheelZoom": true,
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "python.venvFolders": [
    "~/notes"
  ],
  "go.toolsManagement.autoUpdate": true,
  "python.dataScience.askForKernelRestart": false,
  "editor.tabSize": 2,
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "files.associations": {
    "matplotlibrc": "ini"
  },
  "python.dataScience.sendSelectionToInteractiveWindow": true,
  "python.dataScience.alwaysTrustNotebooks": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.formatOnSave": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "todohighlight.keywords": [
    {
      "text": "REFACTOR",
      "color": "#1f1f1f",
      "backgroundColor": "#f58ecc"
    },
    {
      "text": "TESTING",
      "color": "#1f1f1f",
      "backgroundColor": "#c88ef5"
    },
    {
      "text": "REVIEW",
      "color": "#1f1f1f",
      "backgroundColor": "#678ff5"
    },
    {
      "text": "TODO",
      "color": "#1f1f1f",
      "backgroundColor": "#81de84"
    },
    {
      "text": "???",
      "color": "#1f1f1f",
      "backgroundColor": "#f5d94c"
    },
    {
      "text": "!!!",
      "color": "#1f1f1f",
      "backgroundColor": "#f5984c"
    },
    {
      "text": "BROKEN",
      "color": "#1f1f1f",
      "backgroundColor": "#f57067"
    },
  ],
  "cSpell.diagnosticLevel": "Hint",
  "prettier.arrowParens": "avoid",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.quoteStyle": "single",
  "prettier.singleQuote": true,
  "[markdown]": {
    "editor.quickSuggestions": true
  },
  "bracket-pair-colorizer-2.colors": [
    "#3d56f5",
    "#4c84f5",
    "#8ec1f5",
    "#cee8f5",
  ],
  "bracket-pair-colorizer-2.unmatchedScopeColor": "#f53325",
  "editor.matchBrackets": "never",
  "debug.showBreakpointsInOverviewRuler": true,
  "editor.rulers": [
    {
      "column": 69,
      "color": "#141414"
    },
    {
      "column": 79,
      "color": "#141414"
    },
  ],
  "editor.wordWrapColumn": 79,
  "prettier.printWidth": 1000,
  "bracket-pair-colorizer-2.showBracketsInGutter": true,
  "bracket-pair-colorizer-2.showBracketsInRuler": true,
  "editor.highlightActiveIndentGuide": false,
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          "variable",
        ],
        "settings": {
          "foreground": "#ebebeb",
        },
      },
      {
        "scope": [
          "comment",
          "comment punctuation.definition.comment",
          "punctuation.terminator.statement.js",
        ],
        "settings": {
          "foreground": "#4f4f4f",
        },
      },
      {
        "scope": [
          "invalid.illegal"
        ],
        "settings": {
          "foreground": "#f53325"
        }
      },
      {
        "scope": [
          "variable.parameter",
          "constant.character.math.tex",
          "meta.definition.label.latex"
        ],
        "settings": {
          "foreground": "#f57067"
        }
      },
      {
        "scope": [
          "support.function",
          "entity.name.function",
          "markup.italic.emph.latex",
          "constant.other.general.math.tex"
        ],
        "settings": {
          "foreground": "#f5af71"
        }
      },
      {
        "scope": [
          "keyword",
          "storage.type.function.arrow.js",
          "storage.type.function.js",
          "support.class",
          "punctuation.definition.string.begin.latex",
          "punctuation.definition.string.end.latex",
          "constant.numeric.math.tex",
          "constant.language",
        ],
        "settings": {
          "foreground": "#f5984c"
        }
      },
      {
        "scope": [
          "variable.other.constant",
          "variable.object.property.js",
          "meta.var.expr.js",
          "variable.language.this.js",
          "variable.other.declaration.go",
          "variable.other.assignment.go",
        ],
        "settings": {
          "foreground": "#f5e48e"
        }
      },
      {
        "scope": [
          "string",
          "punctuation.definition.string.begin",
          "punctuation.definition.string.end",
        ],
        "settings": {
          "foreground": "#85d688"
        }
      },
      {
        "scope": [
          "entity.name",
          "string.other.link.title.markdown",
          "markup.bold.markdown",
          "markup.underline.link.latex"
        ],
        "settings": {
          "foreground": "#66b068"
        }
      },
      {
        "scope": [
          "punctuation",
          "constant.language.boolean.true.js",
          "constant.other.placeholder.go"
        ],
        "settings": {
          "foreground": "#4cf5d9"
        }
      },
      {
        "scope": [
          "variable.other.readwrite.js",
          "support.variable.property.js",
          "variable.parameter.function.latex",
        ],
        "settings": {
          "foreground": "#bae1f5"
        }
      },
      {
        "scope": [
          "keyword.control",
          "keyword.operator.expression.of.js",
          "keyword.operator.new.js",
          "entity.name.section.latex",
          "keyword.other",
          "support.function.be.latex",
        ],
        "settings": {
          "foreground": "#7abef5",
        }
      },
      {
        "scope": [
          "keyword.operator",
          "support.function.section.latex",
          "punctuation.definition.function.latex",
          "punctuation.definition.function.tex",
          "punctuation.definition.keyword.latex",
          "punctuation.definition.constant.math.tex",
        ],
        "settings": {
          "foreground": "#4c7ff5"
        }
      },
      {
        "scope": [
          "storage.type",
          "storage.type.js",
          "keyword.operator.expression.typeof.js",
          "keyword.operator.assignment.go",
          "keyword.var.go",
        ],
        "settings": {
          "foreground": "#c88ef5"
        }
      },
      {
        "scope": [
          "constant.numeric",
        ],
        "settings": {
          "foreground": "#f5769c"
        }
      },
      {
        "scope": [
          "keyword.operator.logical",
        ],
        "settings": {
          "foreground": "#f54c7f"
        }
      },
      {
        "scope": [
          "entity.name.section.latex",
        ],
        "settings": {
          "fontStyle": "bold"
        }
      },
      {
        "scope": [
          "keyword.operator.expression.of.js",
          "meta.object-literal.key.js",
          "meta.definition.method.js",
          "storage.type"
        ],
        "settings": {
          "fontStyle": "italic",
        }
      },
    ]
  },
  "bracket-pair-colorizer-2.showHorizontalScopeLine": false,
  "editor.wordWrap": "on",
  "bracket-pair-colorizer-2.highlightActiveScope": true,
  "python.formatting.provider": "autopep8",
  "editor.tabCompletion": "on",
}