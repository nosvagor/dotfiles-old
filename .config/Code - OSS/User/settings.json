{
    "workbench.iconTheme": "material-icon-theme",
    "latex-workshop.chktex.enabled": true,
    "latex-workshop.intellisense.package.enabled": true,
    "editor.multiCursorModifier": "ctrlCmd",
    "editor.cursorBlinking": "phase",
    "window.zoomLevel": 0,
    "editor.quickSuggestionsDelay": 0,
    "latex-workshop.latex.clean.fileTypes": [
        "*.aux",
        "*.bbl",
        "*.blg",
        "*.idx",
        "*.ind",
        "*.lof",
        "*.lot",
        "*.out",
        "*.toc",
        "*.acn",
        "*.acr",
        "*.alg",
        "*.glg",
        "*.glo",
        "*.gls",
        "*.fls",
        "*.log",
        "*.fdb_latexmk",
        "*.snm",
        "*.synctex(busy)",
        "*.synctex.gz(busy)",
        "*.nav",
        "*.synctex.gz"
    ],
    "files.exclude": {
        "**/.directory": true
    },
    "explorer.confirmDragAndDrop": false,
    "editor.wordWrap": "on",
    "workbench.sideBar.location": "left",
    "explorer.confirmDelete": false,
    "editor.renderControlCharacters": false,
    "terminal.integrated.inheritEnv": true,
    "workbench.colorCustomizations": {
        "activityBarBadge.background": "#7abef5",
        "activityBar.activeBorder": "#ff9e4f",
        "list.activeSelectionForeground": "#ff9e4f",
        "list.inactiveSelectionForeground": "#ff9e4f",
        "list.highlightForeground": "#ff9e4f",
        "scrollbarSlider.activeBackground": "#ff9e4f50",
        "editorSuggestWidget.highlightForeground": "#ff9e4f",
        "textLink.foreground": "#ff9e4f",
        "progressBar.background": "#ff9e4f",
        "pickerGroup.foreground": "#ff9e4f",
        "notificationLink.foreground": "#ff9e4f",
        "editorWidget.resizeBorder": "#ff9e4f",
        "editorWidget.border": "#ff9e4f",
        "settings.modifiedItemIndicator": "#ff9e4f",
        "settings.headerForeground": "#ff9e4f",
        "panelTitle.activeBorder": "#ff9e4f",
        "breadcrumb.activeSelectionForeground": "#ff9e4f",
        "menu.selectionForeground": "#ff9e4f",
        "menubar.selectionForeground": "#ff9e4f",
        "editor.findMatchBorder": "#ff9e4f",
        "selection.background": "#ff9e4f40",
        "statusBarItem.remoteBackground": "#ff9e4f",
        "tab.activeBorder": "#ff9e4f",
        "tab.activeModifiedBorder": "#ff9e4f",
        "tab.inactiveModifiedBorder": "#b08058",
        "gitDecoration.modifiedResourceForeground": "#b0d0eb",
        "gitDecoration.addedResourceForeground": "#7abef5",
        "gitDecoration.stageModifiedResourceForeground": "#78b07a",
        "gitDecoration.deletedResourceForeground": "#849cb0",
        "gitDecoration.stageDeletedResourceForeground": "#5f89f5",
        "gitDecoration.untrackedResourceForeground": "#f0dd86",
        "[Community Material Theme Darker High Contrast]": {
            "editor.background": "#1f1f1f",
        },
        "tab.border": "#1A1A1A",
        "tab.hoverForeground": "#ff9e4f",
        "editorGroupHeader.tabsBackground": "#1A1A1A",
        "tab.activeBackground": "#1A1A1A",
        "tab.activeForeground": "#ebebeb",
        "tab.inactiveForeground": "#b0b0b0"
    },
    "editor.fontLigatures": true,
    "editor.hover.delay": 142,
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.tabCloseButton": "off",
    "editor.minimap.maxColumn": 42,
    "diffEditor.ignoreTrimWhitespace": false,
    "latex-workshop.message.error.show": false,
    "window.menuBarVisibility": "toggle",
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "latex-workshop.view.pdf.viewer": "external",
    "editor.fontSize": 16,
    "git.enableSmartCommit": true,
    "terminal.integrated.fontFamily": "Hack Nerd Font",
    "latex-workshop.message.update.show": false,
    "latex-workshop.latex.tools": [
        {
          "name": "latexmk",
          "command": "latexmk",
          "args": [
            "-synctex=1",
            "-interaction=nonstopmode",
            "-file-line-error",
            "-lualatex",
            "%DOC%"
          ]
        }
    ],
    "latex-workshop.synctex.afterBuild.enabled": true,
    "editor.fontFamily": "'Hack Nerd Font'",
    "workbench.colorTheme": "Community Material Theme Darker High Contrast",
    "terminal.integrated.shell.linux": "/bin/zsh",
    "terminal.integrated.cursorStyle": "line",
    "terminal.explorerKind": "external",
    "terminal.external.linuxExec": "kitty",
    "editor.mouseWheelZoom": true,
    "telemetry.experiments.showDefaultViewlet": false,
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "python.venvFolders": [
        "notes"
    ],
    "go.toolsManagement.autoUpdate": true,
}
